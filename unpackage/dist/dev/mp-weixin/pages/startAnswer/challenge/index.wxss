.answer {
		/* 设置被选中的按钮的颜色 */
		background-color: #ffab00;
}
#overview {
		animation-name: show;
		animation-duration: 0.3s;
		animation-timing-function: ease-out;
		animation-fill-mode: forwards;
}
@keyframes show {
from {
			right: -1000rpx;
}
to {
			right: 0;
}
}
.hasChosen {
		background-color: #fdd835;
}
#resultPage {
		opacity: 1;
		transition: opacity 1s;
}
